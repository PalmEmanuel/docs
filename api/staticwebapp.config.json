{
    "routes": [
        {
            "route": "/.auth/*",
            "allowedRoles": ["anonymous","authenticated"]
        },
        {
            "route": "/.auth/login/github",
            "allowedRoles": ["anonymous","authenticated"]
        },
        {
            "route": "/login",
            "rewrite": "/.auth/login/github"
        },
        {
            "route": "/*",
            "allowedRoles": ["readers"]
        }
    ],
    "responseOverrides": {
        "401": {
            "redirect": "/login",
            "statusCode": 302
        }
    }
}
